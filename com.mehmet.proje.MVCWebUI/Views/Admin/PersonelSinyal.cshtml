@model PersonelSinyalIslemModel

@{
   
    Layout = "_AdminLayout";
}
<div class="container-fluid">
    <br/>
    <h3 class="bg-warning text-center">@ViewBag.kimlik Tarafından İşlem Yapılan Sinyaller</h3>
    <div class="table-responsive">
        <table class="table table-striped table-hover table-sm">
            <thead class="thead">
            <td>Sid</td>
            <td>Abone No</td>
            <td>Sinyal Tarihi</td>
            <td>Sinyal Saati</td>
            <td>İşl.Saati</td>
            <td>İşl.Tarihi</td>
            <td>Sinyal Kodu</td>
            <td>Bölge No</td>
            <td>Zone</td>
            <td>Operatör</td>
            <td>Sinyal Açıklama</td>
            </thead>
            <tbody>
            @foreach (var sinyal in Model.IslenmisSinyallers)
            {
                if (sinyal.SinyalKod == "E200" ||
                    sinyal.SinyalKod == "E211" ||
                    sinyal.SinyalKod == "E222" ||
                    sinyal.SinyalKod == "E233" ||
                    sinyal.SinyalKod == "E255")
                {
                    <tr class="bg-danger">
                        <td>@sinyal.SinyalId</td>
                        <td>@sinyal.AboneNo</td>
                        <td>@sinyal.SinyalTarih</td>
                        <td>@sinyal.SinyalSaat</td>
                        <td>@sinyal.IslemSaat</td>
                        <td>@sinyal.IslemTarih</td>
                        <td>@sinyal.SinyalKod</td>
                        <td>@sinyal.Bolum</td>
                        <td>@sinyal.Zone</td>
                        <td>@sinyal.OperatorId</td>
                        <td>@sinyal.SinyalTAnim</td>
                    </tr>
                }
                if (sinyal.SinyalKod == "E400" ||
                    sinyal.SinyalKod == "E401" ||
                    sinyal.SinyalKod == "E404")
                {
                    <tr class="bg-success">
                        <td>@sinyal.SinyalId</td>
                        <td>@sinyal.AboneNo</td>
                        <td>@sinyal.SinyalTarih</td>
                        <td>@sinyal.SinyalSaat</td>
                        <td>@sinyal.IslemSaat</td>
                        <td>@sinyal.IslemTarih</td>
                        <td>@sinyal.SinyalKod</td>
                        <td>@sinyal.Bolum</td>
                        <td>@sinyal.Zone</td>
                        <td>@sinyal.OperatorId</td>
                        <td>@sinyal.SinyalTAnim</td>

                    </tr>
                }
                if (sinyal.SinyalKod == "E402")
                {
                    <tr class="bg-warning">
                        <td>@sinyal.SinyalId</td>
                        <td>@sinyal.AboneNo</td>
                        <td>@sinyal.SinyalTarih</td>
                        <td>@sinyal.SinyalSaat</td>
                        <td>@sinyal.IslemSaat</td>
                        <td>@sinyal.IslemTarih</td>
                        <td>@sinyal.SinyalKod</td>
                        <td>@sinyal.Bolum</td>
                        <td>@sinyal.Zone</td>
                        <td>@sinyal.OperatorId</td>
                        <td>@sinyal.SinyalTAnim</td>
                    </tr>
                }
                if (sinyal.SinyalKod == "E403" || sinyal.SinyalKod == "E405")
                {
                    <tr class="bg-dark">
                        <td>@sinyal.SinyalId</td>
                        <td>@sinyal.AboneNo</td>
                        <td>@sinyal.SinyalTarih</td>
                        <td>@sinyal.SinyalSaat</td>
                        <td>@sinyal.IslemSaat</td>
                        <td>@sinyal.IslemTarih</td>
                        <td>@sinyal.SinyalKod</td>
                        <td>@sinyal.Bolum</td>
                        <td>@sinyal.Zone</td>
                        <td>@sinyal.OperatorId</td>
                        <td>@sinyal.SinyalTAnim</td>
                    </tr>
                }
                if (sinyal.SinyalKod == "E140")
                {
                    <tr class="bg-secondary">
                        <td>@sinyal.SinyalId</td>
                        <td>@sinyal.AboneNo</td>
                        <td>@sinyal.SinyalTarih</td>
                        <td>@sinyal.SinyalSaat</td>
                        <td>@sinyal.IslemSaat</td>
                        <td>@sinyal.IslemTarih</td>
                        <td>@sinyal.SinyalKod</td>
                        <td>@sinyal.Bolum</td>
                        <td>@sinyal.Zone</td>
                        <td>@sinyal.OperatorId</td>
                        <td>@sinyal.SinyalTAnim</td>
                    </tr>
                }
                if (sinyal.SinyalKod == "E120")
                {
                    <tr class="bg-info">
                        <td>@sinyal.SinyalId</td>

                        <td>@sinyal.AboneNo</td>

                        <td>@sinyal.SinyalTarih</td>

                        <td>@sinyal.SinyalSaat</td>

                        <td>@sinyal.IslemSaat</td>

                        <td>@sinyal.IslemTarih</td>
                        <td>@sinyal.SinyalKod</td>
                        <td>@sinyal.Bolum</td>
                        <td>@sinyal.Zone</td>
                        <td>@sinyal.OperatorId</td>
                        <td>@sinyal.SinyalTAnim</td>

                    </tr>
                }
                if (sinyal.SinyalKod == "E100")
                {
                    <tr class="bg-dark">
                        <td>@sinyal.SinyalId</td>
                        <td>@sinyal.AboneNo</td>
                        <td>@sinyal.SinyalTarih</td>
                        <td>@sinyal.SinyalSaat</td>
                        <td>@sinyal.IslemSaat</td>
                        <td>@sinyal.IslemTarih</td>
                        <td>@sinyal.SinyalKod</td>
                        <td>@sinyal.Bolum</td>
                        <td>@sinyal.Zone</td>
                        <td>@sinyal.OperatorId</td>
                        <td>@sinyal.SinyalTAnim</td>

                    </tr>
                }
            }
            </tbody>
        </table>

    </div>
</div>


