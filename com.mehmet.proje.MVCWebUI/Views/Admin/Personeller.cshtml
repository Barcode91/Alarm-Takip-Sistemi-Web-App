@model AdminPersonelModel

@{
    Layout = "_AdminLayout";
}
<div class="container-fluid text-center">
    <h3 class="bg-warning">Abone Listesi</h3>
    <div class="table-responsive">
        <table class="table table-striped table-hover table-sm ">
            <thead class="thead">
            <td>Personel No</td>
            <td>Tc Kimlik No</td>
            <td>Adı Soyadı</td>
            <td>Cep Telefonu</td>
            <td>Görev Türü</td>
           
            <td></td>
            <td></td>
            <td></td>


            </thead>
            <tbody>
            @foreach (var personel in Model._personels)
            {
                <tr>
                    <td >@personel.PersonelId</td>
                    <td>@personel.PersonelTcNo</td>
                    <td>@personel.Kimlik</td>
                    <td>@personel.CepTel</td>
                    <td>@personel.GorevTur</td>
                    <td><a class="btn btn-sm btn-info" asp-controller="Admin" asp-action="PersonelBilgi" asp-route-musteriId="@personel.PersonelId">Göster</a></td>
                    <td><a class="btn btn-sm btn-primary" asp-controller="Admin" asp-action="PersonelGuncelle" asp-route-musteriId="@personel.PersonelId">Güncelle</a></td>
                    <td><a class="btn btn-sm btn-danger" asp-controller="Admin" asp-action="PersonelSil" asp-route-musteriId="@personel.PersonelId">Sil</a></td>




                </tr>


            }

            </tbody>
        </table>




    </div>

</div>